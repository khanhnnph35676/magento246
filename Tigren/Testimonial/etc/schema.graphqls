type Query {
    getTestimonialById(id: Int!): Testimonial @resolver(class: "\\Tigren\\Testimonial\\Model\\Resolver\\GetTestimonialById")
}

type Testimonial {
    entity_id: Int
    customer_id: Int
    name: String
    email: String
    message: String
    company: String
    rating: Int
    profile_image: String
    status: Int
    created_at: String
}
